   "source": [
    "lst = []\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "lst.append(N)\n",
    "\n",
    "while(True):\n",
    "    \n",
    "    ten = lst[-1]//10\n",
    "    one = lst[-1]%10\n",
    "    sum = ten + one\n",
    "    \n",
    "    lst.append(one*10 + sum%10)\n",
    "    \n",
    "    if N == lst[-1]:\n",
    "        break\n",
    "        \n",
    "print(len(lst)-1)"
   ]
